@model IEnumerable<TestApp.Web.Models.UsersEntry>


<div ng-controller="app.views.users as vm">    
       
    <table>

        <tr>
            <th>
                 Имя
            </th>
            <th>
                Логин
            </th>
            <th>
                Пароль
            </th>
            <th>
               Почта
            </th>
            <th></th>
        </tr>
        @foreach (var user in (IEnumerable<TestApp.Web.Models.UsersEntry>)@TempData["Greeting"])
        {
            <tr>
                <td>
                   @user.name
                </td>
                <td>
                    @user.login
                </td>
                <td>
                   @user.password
                </td>
                <td>
                    @user.email
                </td>

            </tr>
        }
    </table>
        
</div>